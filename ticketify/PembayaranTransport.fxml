<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" fx:id="pembayaran" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="ticketify.PembayaranTransportController">
   <children>
      <Label layoutX="224.0" layoutY="14.0" text="PEMBAYARAN">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="kembali" layoutX="14.0" layoutY="347.0" mnemonicParsing="false" onAction="#kembali" text="Kembali">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="158.0" layoutY="71.0" text="Jumlah Tiket" />
      <Label layoutX="158.0" layoutY="108.0" text="Jenis Pembayaran" />
      <TextField id="jumlahTiket" fx:id="jumlahTiket" layoutX="291.0" layoutY="67.0" prefHeight="25.0" prefWidth="172.0" text="Jumlah tiket yang ingin dibeli" />
      <MenuButton layoutX="291.0" layoutY="104.0" mnemonicParsing="false" text="MenuButton">
         <items>
            <MenuItem id="Bank" fx:id="Bank" mnemonicParsing="false" onAction="#bank" text="Bank" />
            <MenuItem id="E-Wallet" fx:id="EWallet" mnemonicParsing="false" onAction="#ewallet" text="E-Wallet" />
         </items>
      </MenuButton>
      <Button id="bayar" fx:id="Bayar" layoutX="429.0" layoutY="171.0" mnemonicParsing="false" onAction="#bayar" text="Bayar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
